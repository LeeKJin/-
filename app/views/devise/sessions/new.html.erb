<center>

<%= simple_form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>


<div class="login" style="font-family:'Hanna'; font-size:30px";>
  
  <form class="fHanna">
    <%= f.input :email, required: false, autofocus: true %> <br>
     <%= f.input :password, required: false %>
     <br>
    <%= f.button :submit, t(".sign_in") %>
  </form>
</div>

<% end %>
<div class="fHanna">
<%= render "devise/shared/links" %>
</div>
</center> 
